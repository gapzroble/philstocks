{
    "name": "philstocks chrome extension",
    "description": "my chrome ext",
    "version": "0.1",
    "permissions": [
        "https://www.philstocks.ph/"
    ],
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["https://www.philstocks.ph/Infinity/Default.cshtml"],
            "css": [
                "intracandle/style.css",
                "jqChartJQueryPlugin/css/jquery.jqChart.css",
                "jqChartJQueryPlugin/css/jquery.jqRangeSlider.css",
                "jqChartJQueryPlugin/themes/smoothness/jquery-ui-1.10.4.css"
            ],
            "js": [
                "jqChartJQueryPlugin/js/jquery-1.11.1.min.js",
                "jqChartJQueryPlugin/js/jquery.mousewheel.js",
                "jqChartJQueryPlugin/js/jquery.jqChart.min.js",
                "jqChartJQueryPlugin/js/jquery.jqRangeSlider.min.js",
                "intracandle/main.js"
            ]
        }
    ]
}
