{
    "name": "philstocks chrome extension",
    "description": "my chrome ext",
    "version": "0.1",
    "permissions": [
        "https://www.philstocks.ph/"
    ],
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["https://www.philstocks.ph/Infinity/Default.cshtml"],
            "css": [
                "assets/jqChartJQueryPlugin/css/jquery.jqChart.css",
                "assets/jqChartJQueryPlugin/css/jquery.jqRangeSlider.css",
                "assets/jqChartJQueryPlugin/themes/smoothness/jquery-ui-1.10.4.css",
                "assets/style.css"
            ],
            "js": [
                "assets/jqChartJQueryPlugin/js/jquery-1.11.1.min.js",
                "assets/jqChartJQueryPlugin/js/jquery.mousewheel.js",
                "assets/jqChartJQueryPlugin/js/jquery.jqChart.min.js",
                "assets/jqChartJQueryPlugin/js/jquery.jqRangeSlider.min.js",
                "assets/date.format.js",
                "assets/main.js"
            ]
        }
    ]
}
